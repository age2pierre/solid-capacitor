{
  "name": "capacitor-solid-template",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "packageManager": "pnpm@9.1.2",
  "engines": {
    "node": "20.12.x"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "tsx ./src/entry-server.ts",
    "clean": "rm -rf dist",
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "vite build",
    "build:server": "vite build --ssr",
    "format": "prettier --write . --ignore-unknown",
    "typecheck": "tsc --noEmit",
    "serve:vite": "vite preview",
    "serve": "NODE_ENV=production node ./dist/server/entry-server.js",
    "serve:debug": "NODE_ENV=production node --inspect-brk ./dist/server/entry-server.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/serve-handler": "6.1.4",
    "prettier": "3.3.3",
    "typescript": "5.5.4",
    "vite": "5.4.2",
    "vite-plugin-inspect": "0.8.7",
    "vite-plugin-solid": "2.10.2",
    "tsx": "4.19.0"
  },
  "dependencies": {
    "@mobily/ts-belt": "3.13.1",
    "express": "4.19.2",
    "serve-handler": "6.1.5",
    "solid-js": "1.8.22",
    "telefunc": "0.1.78"
  }
}
