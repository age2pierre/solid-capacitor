{
  "name": "capacitor-solid-template",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "packageManager": "pnpm@9.1.2",
  "engines": {
    "node": "20.12.x"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "tsx ./src/entry-server.ts",
    "clean": "rm -rf dist",
    "build": "pnpm run build:client && run pnpm build:server",
    "build:client": "vite build",
    "build:server": "vite build --ssr",
    "format": "prettier --write . --ignore-unknown",
    "format:check": "prettier --check . --ignore-unknown",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src/",
    "lint:fix": "eslint --fix ./src/",
    "serve": "NODE_ENV=production node ./dist/server/entry-server.js",
    "android": "cap open android"
  },
  "license": "MIT",
  "devDependencies": {
    "@capacitor/cli": "6.1.2",
    "@eslint/js": "9.9.1",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/eslint__js": "8.42.3",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.6",
    "@types/serve-handler": "6.1.4",
    "autoprefixer": "10.4.20",
    "eslint": "9.9.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "postcss": "8.4.44",
    "prettier": "3.3.3",
    "tailwindcss": "3.4.10",
    "typescript": "5.5.4",
    "typescript-eslint": "8.4.0",
    "vite": "5.4.2",
    "vite-plugin-inspect": "0.8.7",
    "vite-plugin-solid": "2.10.2"
  },
  "dependencies": {
    "@capacitor/android": "6.1.2",
    "@capacitor/core": "6.1.2",
    "@capacitor/ios": "6.1.2",
    "@solidjs/router": "0.14.3",
    "@tanstack/solid-virtual": "3.10.6",
    "express": "4.19.2",
    "jsonwebtoken": "9.0.2",
    "serve-handler": "6.1.5",
    "solid-js": "1.8.22",
    "telefunc": "0.1.78",
    "tsx": "4.19.0"
  }
}
