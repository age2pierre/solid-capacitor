{
  "name": "capacitor-solid-template",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "packageManager": "pnpm@9.1.2",
  "engines": {
    "node": "20.12.x"
  },
  "scripts": {
    "android": "cap open android",
    "build": "pnpm run build:client && pnpm run build:server && pnpm run build:sync",
    "build:assets": "capacitor-assets generate",
    "build:client": "vite build",
    "build:server": "vite build --ssr",
    "build:sync": "cap sync",
    "clean": "rm -rf dist",
    "db:generate": "pnpm with-env drizzle-kit generate --config src/db/config.ts",
    "db:push": "pnpm with-env drizzle-kit push:pg --config src/db/config.ts",
    "db:studio": "pnpm with-env drizzle-kit studio --config src/db/config.ts",
    "dev": "pnpm run build:server && node --env-file=.env.local ./dist/server/entry-server.js",
    "format": "prettier --write . --ignore-unknown",
    "format:check": "prettier --check . --ignore-unknown",
    "lint": "eslint ./src/",
    "lint:fix": "eslint --fix ./src/",
    "preinstall": "npx only-allow pnpm",
    "prepare": "ts-patch install && typia patch",
    "serve": "node --env-file=.env ./dist/server/entry-server.js",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "license": "MIT",
  "imports": {
    "#*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*.js",
      "./src/*.jsx",
      "./src/*/index.ts",
      "./src/*/index.tsx",
      "./src/*/index.js",
      "./src/*/index.jsx"
    ]
  },
  "devDependencies": {
    "@capacitor/assets": "3.0.5",
    "@capacitor/cli": "6.1.2",
    "@eslint/js": "9.9.1",
    "@ryoppippi/unplugin-typia": "npm:@jsr/ryoppippi__unplugin-typia@1.0.3-beta.4",
    "@types/eslint__js": "8.42.3",
    "@types/multer": "1.4.12",
    "autoprefixer": "10.4.20",
    "dotenv-cli": "7.4.2",
    "drizzle-kit": "0.24.2",
    "eslint": "9.9.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-solid": "0.14.3",
    "postcss": "8.4.44",
    "prettier": "3.3.3",
    "solid-devtools": "0.30.1",
    "tailwindcss": "3.4.10",
    "ts-patch": "3.2.1",
    "typescript": "5.5.4",
    "typescript-eslint": "8.4.0",
    "vite": "5.4.2",
    "vite-plugin-inspect": "0.8.7",
    "vite-plugin-solid": "2.10.2"
  },
  "dependencies": {
    "@capacitor/android": "6.1.2",
    "@capacitor/core": "6.1.2",
    "@capacitor/ios": "6.1.2",
    "@kobalte/core": "0.13.6",
    "@mobily/ts-belt": "3.13.1",
    "@solid-devtools/overlay": "0.30.1",
    "@solidjs/router": "0.14.3",
    "@tanstack/solid-virtual": "3.10.6",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.6",
    "@types/serve-handler": "6.1.4",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "cors": "2.8.5",
    "drizzle-orm": "0.33.0",
    "express": "4.19.2",
    "multer": "1.4.5-lts.1",
    "jsonwebtoken": "9.0.2",
    "pg": "8.12.0",
    "postgres": "3.4.4",
    "postgres-large-object": "1.0.5",
    "serve-handler": "6.1.5",
    "sharp": "0.33.5",
    "solid-js": "1.8.22",
    "tailwind-merge": "2.5.2",
    "tailwindcss-animate": "1.0.7",
    "telefunc": "0.1.78",
    "type-fest": "4.26.1",
    "typia": "6.9.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "@mobily/ts-belt": "patches/@mobily__ts-belt.patch"
    }
  }
}
